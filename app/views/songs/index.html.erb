<h1>Songs</h1>

<ul>
  <% @songs.each do |song| %>
    <li>
      <a href="<%= song_path(song)%>">
        <%= song.artist_name %>
        -
        <%= song.title %>
      </a>
    </li>
    <p>
      <%= link_to song.artist_name, artist_path(song.artist) if song.artist %>
    </p>
  <% end %>
</ul>
